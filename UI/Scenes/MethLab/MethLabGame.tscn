[sub_resource type="Shader" id="Shader_pqxe6"]
code = "shader_type canvas_item;

uniform float brightness = 0.8;
uniform float scan_line_count = 100.0;
uniform float scan_line_strength = 0.1;

void fragment() {
    // Get the original color
    vec4 original_color = texture(TEXTURE, UV);
    
    // Apply scanlines
    float scan_line = sin(UV.y * scan_line_count * 3.14159);
    vec4 scan_effect = mix(original_color, vec4(0.0, 0.0, 0.0, original_color.a), scan_line_strength * abs(scan_line));
    
    // Apply brightness adjustment
    vec4 final_color = scan_effect * brightness;
    
    COLOR = final_color;
}"

